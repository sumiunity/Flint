{
  "name": "Flint",
  "version": "1.0.0",
  "description": "Generates a boiler plate for react application development",
  "main": "src/index.js",
  "scripts": {
    "build": "babel index.js -d dist",
    "start": "npx -p @babel/core -p @babel/node babel-node --presets @babel/preset-env src/index.js",
    "install": "npx -p @babel/core -p @babel/node babel-node --presets @babel/preset-env src/index.js",
    "monitor": "nodemon --exec ./node_modules/.bin/babel-node src/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
